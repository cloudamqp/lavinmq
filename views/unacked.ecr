<%- pagename = "Unacked" -%>
<!DOCTYPE html>
<html lang="en">
  <head>
    <% render "partials/head" %>
    <script type="module" src="js/unacked.js"></script>
  </head>
  <body>
    <% render "partials/header" %>
    <div class="grid-content">
      <main class="unacked-main">
        <section class="card rounded-xl bg-dark-900 px-6 pb-6 pt-4 border-light">
          <h3>
            <a class="text-lg font-semibold text-light-400" href="" id="queue-link"></a>
            <small id="unacked-count"></small>
          </h3>
          <div class="table-wrapper">
            <div id="table-error"></div>
            <table id="table" class="table text-light-400">
              <thead>
                <tr>
                  <th class="left">Message tag</th>
                  <th class="left">Consumer tag</th>
                  <th class="left">Channel</th>
                  <th class="right">Seconds unacked</th>
                </tr>
              </thead>
              <tbody></tbody>
            </table>
          </div>
        </section>
      </main>
      <% render "partials/footer" %>
    </div>
  </body>
</html>
